{
    "manifest_version": 3,
    "name": "RC-Oracle",
    "version": "1.0.0",
    "description": "Hybrid Symbolic-Neural Solver for Reverse Coding challenges",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://localhost:8000/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "styles.css"
            ],
            "run_at": "document_idle"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "injector.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "RC-Oracle"
    },
    "icons": {}
}